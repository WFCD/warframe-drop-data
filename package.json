{
  "name": "warframe-drop-data",
  "version": "0.0.1",
  "description": "Converts the official Warframe drop data into something useable.",
  "main": "generate.js",
  "scripts": {
    "start": "node generateData.js",
    "serve": "npx http-server site -p $PORT",
